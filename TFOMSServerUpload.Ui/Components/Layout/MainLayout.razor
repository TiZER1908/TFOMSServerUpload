@inherits LayoutComponentBase


<TelerikRootComponent>
    <div class="page">
        <div class="fixed-top">
            <Header />
        </div>

        <main>
            <div style="height: 60px">
            </div>

            <article class="content">
                @Body
            </article>
        </main>
    </div>

    <div id="blazor-error-ui">
        An unhandled error has occurred.
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
</TelerikRootComponent>

